generator client {
  provider  = "prisma-client-js"
  output    = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Url {
  id        Int  @id @default(autoincrement())
  name      String
  slug      String @unique
  url       String
  clicks    Int     @default(0)
}
